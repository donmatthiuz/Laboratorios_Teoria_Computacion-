digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	3
	node [shape=circle]
	"" [style=invis]
	"" -> 0
	0 -> 1 [label=a]
	"" [style=invis]
	"" -> 0
	0 -> 2 [label=b]
	1 -> 2 [label=a]
	1 -> 3 [label=b]
	2 -> 2 [label=a]
	2 -> 2 [label=b]
	3 -> 2 [label=a]
	3 -> 2 [label=b]
}
