digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	"[4]"
	node [shape=circle]
	"[4]" -> "[3]" [label=a]
	"[4]" -> "[3]" [label=b]
	"[2]" -> "[3]" [label=a]
	"[2]" -> "[4]" [label=b]
	"[3]" -> "[3]" [label=a]
	"[3]" -> "[3]" [label=b]
	"" [style=invis]
	"" -> "[0, 1]"
	"[0, 1]" -> "[0, 1]" [label=a]
	"" [style=invis]
	"" -> "[0, 1]"
	"[0, 1]" -> "[2]" [label=b]
}
