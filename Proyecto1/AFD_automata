digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	1
	5
	node [shape=circle]
	"" [style=invis]
	"" -> 0
	0 -> 1 [label=1]
	"" [style=invis]
	"" -> 0
	0 -> 2 [label=0]
	1 -> 3 [label=1]
	1 -> 4 [label=0]
	2 -> 1 [label=1]
	2 -> 2 [label=0]
	3 -> 3 [label=1]
	3 -> 3 [label=0]
	4 -> 5 [label=1]
	4 -> 6 [label=0]
	5 -> 3 [label=1]
	5 -> 4 [label=0]
	6 -> 5 [label=1]
	6 -> 6 [label=0]
}
