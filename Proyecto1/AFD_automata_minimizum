digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	"[4]"
	node [shape=circle]
	"[4]" -> "[1, 0]" [label=b]
	"[4]" -> "[2]" [label=a]
	"[3]" -> "[4]" [label=b]
	"[3]" -> "[2]" [label=a]
	"" [style=invis]
	"" -> "[1, 0]"
	"[1, 0]" -> "[1, 0]" [label=b]
	"" [style=invis]
	"" -> "[1, 0]"
	"[1, 0]" -> "[2]" [label=a]
	"[2]" -> "[3]" [label=b]
	"[2]" -> "[2]" [label=a]
}
