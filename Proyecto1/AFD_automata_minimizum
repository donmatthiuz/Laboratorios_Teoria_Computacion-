digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=doublecircle]
	"[1, 4]"
	"[0]"
	"[2]"
	node [shape=circle]
	"[3]" -> "[3]" [label=1]
	"[3]" -> "[3]" [label=0]
	"[1, 4]" -> "[3]" [label=1]
	"[1, 4]" -> "[1, 4]" [label=0]
	"" [style=invis]
	"" -> "[0]"
	"[0]" -> "[1, 4]" [label=1]
	"" [style=invis]
	"" -> "[0]"
	"[0]" -> "[2]" [label=0]
	"[2]" -> "[1, 4]" [label=1]
	"[2]" -> "[1, 4]" [label=0]
}
